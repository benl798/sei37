<%= form_for @artist do |f| %>

  <%= f.label :name %><br>
  <%= f.text_field :name %>
  <br>

  <%= f.label :nationality %><br>
  <%= f.text_field :nationality %>
  <br>

  <%= f.label :dob %><br>
  <%= f.text_field :dob %>
  <br>

  <%= f.label :period %><br>
  <%= f.text_field :period %>
  <br>

  <%= f.label :roundness %><br>
  <%= f.text_field :roundness %>
  <br>

  <%= f.label :bio %><br>
  <%= f.text_field :bio %>
  <br>

  <%= f.label :image, 'Image URL' %><br>
  <%= f.text_field :image %>

  <br>
  <br>

  <%= f.submit %>

<% end %>
